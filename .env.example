# .env.example
VITE_APP_NAME=Property Admin Dashboard
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_WEBSOCKET_URL=ws://localhost:8000

# Authentication
VITE_JWT_SECRET=your-jwt-secret-here
VITE_JWT_EXPIRES_IN=24h

# Third-party Integrations
VITE_NIN_VERIFICATION_API_KEY=your-nin-api-key
VITE_BVN_VERIFICATION_API_KEY=your-bvn-api-key

# Payment Gateway
VITE_PAYSTACK_PUBLIC_KEY=your-paystack-public-key
VITE_FLUTTERWAVE_PUBLIC_KEY=your-flutterwave-public-key

# Google Services
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Email Service
VITE_SENDGRID_API_KEY=your-sendgrid-api-key

# SMS Service
VITE_TWILIO_ACCOUNT_SID=your-twilio-sid
VITE_TWILIO_AUTH_TOKEN=your-twilio-token

# File Upload
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-name
VITE_CLOUDINARY_API_KEY=your-cloudinary-key

# Monitoring
VITE_SENTRY_DSN=your-sentry-dsn
VITE_ANALYTICS_ID=your-analytics-id

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true

# Development
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info